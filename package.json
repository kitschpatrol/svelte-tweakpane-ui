    "@types/eslint": "^8.56.10",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.2",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.1",
    "npm-run-all": "^4.1.5",
