    "tslib": "^2.6.3",
    "tsx": "^4.15.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.13",
    "yaml": "^2.4.5"
  },
  "pnpm": {
    "patchedDependencies": {
      "@astrojs/starlight@0.24.0": "patches/@astrojs__starlight@0.24.0.patch"
    }
  }
}
