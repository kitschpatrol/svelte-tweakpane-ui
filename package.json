    "publint": "^0.2.8",
    "read-package-up": "^11.0.0",
    "remark-mdat": "^0.7.0",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.0",
    "svelte-language-server": "^0.16.10",
    "svelte2tsx": "^0.7.9",
